# Database Configuration
# Copy this file to .env and update the values for your local development

POSTGRES_DB=soundsphere
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here

# JWT Configuration
# Generate a secure Base64-encoded secret key (minimum 256 bits / 32 bytes)
# You can generate one using: openssl rand -base64 32
JWT_SECRET=your_base64_encoded_secret_key_here
